{
  "name": "braunstetter/media-bundle",
  "description": "Everything you need in order to upload and manage media files with symfony.",
  "type": "symfony-bundle",
  "keywords": ["media", "symfony-ux", "symfony"],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "symfony/framework-bundle": "^4.4|^5.0|^6.0",
    "symfony/string": "^6.0",
    "webmozart/assert": "^1.1",
    "braunstetter/helper" : "^0",
    "symfony/mime": "^6.0",
    "doctrine/orm": "^2.11",
    "symfony/form": "^6.0",
    "symfony/asset": "^6.0"
  },
  "require-dev": {
    "symfony/test-pack": "^1.0",
    "matthiasnoback/symfony-dependency-injection-test": "^4.2",
    "nyholm/symfony-bundle-test": "1.8.1",
    "doctrine/doctrine-bundle": "^2.5",
    "symplify/amnesia": "^10.2",
    "liip/imagine-bundle": "^2.7",
    "symfony/twig-bundle": "^4.4|^5.0|^6.0",
    "symfony/panther": "^2.0",
    "dbrekelmans/bdi": "^1.0",
    "symfony/debug-bundle": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Braunstetter\\MediaBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Braunstetter\\MediaBundle\\Tests\\": "tests/tests",
      "App\\": "tests/app/src"
    }
  }
}
